{
  "name": "@ticobot/backend",
  "version": "0.1.0",
  "type": "module",
  "description": "Backend API and data processing for TicoBot",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:server": "tsx watch src/api/server.ts",
    "start": "node dist/api/server.js",
    "start:server": "tsx src/api/server.ts",
    "test:ingestion": "tsx src/scripts/testIngestion.ts",
    "admin:create": "tsx scripts/create-admin.ts",
    "build": "tsc",
    "test": "vitest --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "echo '⚠️  Skipping backend lint' && exit 0",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "ticobot",
    "backend",
    "rag"
  ],
  "author": "juanpredev",
  "license": "MIT",
  "packageManager": "pnpm@10.22.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "@ticobot/shared": "workspace:*",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "openai": "^4.28.0",
    "pdf-parse": "^2.4.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tiktoken": "^1.0.22",
    "zod": "^3.22.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cheerio": "^1.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.15.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/zxcvbn": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "pg": "^8.16.3",
    "supabase": "^2.65.6",
    "tsx": "^4.20.6",
    "typescript": "^5.4.0",
    "vitest": "^1.4.0"
  }
}
